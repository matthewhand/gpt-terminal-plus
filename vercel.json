{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "USE_SERVERLESS": "true"
  },
  "functions": {
    "dist/index.js": {
      "maxDuration": 30
    }
  }
}