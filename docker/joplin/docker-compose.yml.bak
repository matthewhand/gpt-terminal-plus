services:
  app:
    build:
      context: ../..
      dockerfile: joplin/Dockerfile
    container_name: joplin-cli-app
    ports:
      - "${JOPLIN_CLI_PORT_HOST:-5008}:${JOPLIN_CLI_PORT_CONTAINER:-5004}"
    env_file:
      - ../../.env
    environment:
      - NODE_ENV=${NODE_ENV}
      - DEBUG=${DEBUG}
      - API_TOKEN=${API_TOKEN}
    restart: unless-stopped
