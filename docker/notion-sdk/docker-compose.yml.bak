services:
  app:
    build:
      context: ../..
      dockerfile: notion-sdk/Dockerfile
    container_name: notion-sdk-app
    ports:
      - "${NOTION_SDK_PORT_HOST:-5009}:${NOTION_SDK_PORT_CONTAINER:-5004}"
    env_file:
      - ../../.env
    environment:
      - NODE_ENV=${NODE_ENV}
      - DEBUG=${DEBUG}
      - API_TOKEN=${API_TOKEN}
    restart: unless-stopped
