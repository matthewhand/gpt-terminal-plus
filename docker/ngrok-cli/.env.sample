# .env.sample for ngrok-cli
API_TOKEN=your_secure_api_token
NODE_ENV=production
DEBUG=app:* 

# Host port to bind to (default: 5013)
NGROK_CLI_PORT_HOST=5013

# Container port (usually 5005, should remain unchanged)
NGROK_CLI_PORT_CONTAINER=5005

# Your ngrok authentication token
NGROK_AUTH_TOKEN=your-ngrok-auth-token

# Region for ngrok (e.g., us, eu, ap, au)
#NGROK_REGION=us


# Ngrok settings
NGROK_ENABLED=false
NGROK_PORT=8080
NGROK_AUTHTOKEN=your-ngrok-authtoken
BASE_URI=https://yourdomain.com/
APP_UNIQUE_ID=gpt-terminal-plus

