{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "static": {
    "include": ["dist"]
  },
  "env": {
    "API_TOKEN": "@API_TOKEN",
    "NODE_ENV": "@NODE_ENV",
    "NODE_CONFIG_DIR": "@NODE_CONFIG_DIR",
    "DEBUG": "@DEBUG"
  }
}
