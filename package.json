{
  "name": "gpt-terminal-plus",
  "version": "latest",
  "description": "ChatGPT terminal action for Custom GPT",
  "main": "dist/index.js",
  "bin": {
    "jit": "dist/index.js"
  },
  "type": "commonjs",
  "scripts": {
    "lint": "npx eslint src/",
    "check-types": "tsc --noEmit",
    "version": "tsc -v",
    "build": "./node_modules/.bin/tsc",
    "start": "node --max-old-space-size=256 dist/index.js",
    "start:dev": "nodemon --exec npx ts-node src/index.ts",
    "validate": "npm-run-all --parallel lint test",
    "pretest": "rm -rf dist",
    "test": "NODE_CONFIG_DIR=config/test/ NODE_ENV=test node ./node_modules/jest/bin/jest.js --runInBand",
    "test:coverage": "NODE_CONFIG_DIR=config/test/ NODE_ENV=test node ./node_modules/jest/bin/jest.js --coverage --runInBand",
    "test:watch": "NODE_CONFIG_DIR=config/test/ NODE_ENV=test node ./node_modules/jest/bin/jest.js --watch",
    "test:ci": "NODE_CONFIG_DIR=config/test/ NODE_ENV=test node ./node_modules/jest/bin/jest.js --coverage --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:debug": "NODE_CONFIG_DIR=config/test/ NODE_ENV=test DEBUG=* node ./node_modules/jest/bin/jest.js --verbose --runInBand",
    "test:monitor": "NODE_CONFIG_DIR=config/test/ NODE_ENV=test DEBUG=* node ./node_modules/jest/bin/jest.js --verbose --runInBand --watch",
    "test:detached": "bash -c 'nohup npm test > test-results.log 2>&1 & echo $! > .jest.pid; echo \"Started Jest in background (PID $(cat .jest.pid))\"'",
    "test:stop": "bash -c 'test -f .jest.pid && kill $(cat .jest.pid) 2>/dev/null || true; rm -f .jest.pid'",
    "showcase": "bash scripts/capture_showcase.sh",
    "ncu": "ncu",
    "smoke": "bash scripts/smoke.sh",
    "openapi:generate": "node scripts/generate-openapi.cjs",
    "postbuild": "npm run openapi:generate",
    "prestart": "npm run openapi:generate"
  },
  "keywords": [],
  "author": "matthewhand+dot+au+at+gmail+dot+com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@eslint/eslintrc": "^3.1.0",
    "@types/body-parser": "^1.19.5",
    "@types/chai": "^4.3.20",
    "@types/config": "^3.3.4",
    "@types/convict": "^6.1.6",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.1.0",
    "@types/proper-lockfile": "^4.1.4",
    "@types/sinon": "^17.0.3",
    "@types/ssh2": "^1.15.0",
    "@types/ssh2-sftp-client": "^9.0.3",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@yao-pkg/pkg": "^6.6.0",
    "babel-jest": "^29.7.0",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "natural-compare": "^1.4.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.665.0",
    "@eslint/config-array": "^0.17.1",
    "@eslint/object-schema": "^2.1.4",
    "@modelcontextprotocol/sdk": "^1.4.1",
    "@ngrok/ngrok": "^1.4.1",
    "@types/shell-escape": "^0.2.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/ws": "^8.18.1",
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "config": "^3.3.12",
    "convict": "^6.2.4",
    "cors": "^2.8.5",
    "debug": "^4.3.6",
    "diff": "^8.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "glob": "^11.0.0",
    "lru-cache": "^11.0.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "openai": "^5.12.2",
    "proper-lockfile": "^4.1.2",
    "rimraf": "^6.0.1",
    "serverless-http": "^3.2.0",
    "shell-escape": "^0.2.0",
    "shescape": "^2.1.1",
    "ssh2": "^1.15.0",
    "ssh2-sftp-client": "^10.0.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "swagger-jsdoc": "^6.2.8",
    "url": "^0.11.4",
    "uuid": "^10.0.0",
    "ws": "^8.18.3",
    "yaml": "^2.8.1",
    "zod": "^3.24.1"
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js"
    ],
    "assets": [
      "README.md",
      "TODO.md",
      "public/**/*",
      "docs/**/*",
      "config/**/*"
    ]
  },
  "_moduleAliases": {
    "@src": "src"
  }
}