{
  "name": "gpt-terminal-plus",
  "version": "latest",
  "description": "ChatGPT terminal action for Custom GPT",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
    "lint": "npx eslint src/",
    "check-types": "tsc --noEmit",
    "version": "tsc -v",
    "build": "tsc",
    "start": "node --max-old-space-size=256 dist/index.js",
    "start:dev": "nodemon --exec npx ts-node src/index.ts",
    "validate": "npm-run-all --parallel lint test",
    "pretest": "rm -rf dist",
    "test": "cross-env NODE_CONFIG_DIR=config/test/ NODE_ENV=test jest --runInBand",
    "test:coverage": "cross-env NODE_CONFIG_DIR=config/test/ NODE_ENV=test jest --coverage --runInBand",
    "test:debug": "cross-env NODE_CONFIG_DIR=config/test/ NODE_ENV=test DEBUG=* jest --verbose --runInBand",
    "test:monitor": "npx cross-env NODE_CONFIG_DIR=config/test/ NODE_ENV=test DEBUG=* jest --verbose --runInBand --watch",
    "showcase": "bash scripts/capture_showcase.sh",
    "ncu": "ncu"
  },
  "keywords": [],
  "author": "matthewhand+dot+au+at+gmail+dot+com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@eslint/eslintrc": "^3.1.0",
    "@types/body-parser": "^1.19.5",
    "@types/chai": "^4.3.20",
    "@types/config": "^3.3.4",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.1.0",
    "@types/proper-lockfile": "^4.1.4",
    "@types/sinon": "^17.0.3",
    "@types/ssh2": "^1.15.0",
    "@types/ssh2-sftp-client": "^9.0.3",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "babel-jest": "^29.7.0",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "natural-compare": "^1.4.0",
    "sinon": "^19.0.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.665.0",
    "@eslint/config-array": "^0.17.1",
    "@eslint/object-schema": "^2.1.4",
    "@modelcontextprotocol/sdk": "^1.4.1",
    "@ngrok/ngrok": "^1.4.1",
    "@types/shell-escape": "^0.2.3",
    "body-parser": "^1.20.2",
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "debug": "^4.3.6",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "glob": "^11.0.0",
    "lru-cache": "^11.0.0",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "proper-lockfile": "^4.1.2",
    "rimraf": "^6.0.1",
    "serverless-http": "^3.2.0",
    "shell-escape": "^0.2.0",
    "shescape": "^2.1.1",
    "ssh2": "^1.15.0",
    "ssh2-sftp-client": "^10.0.3",
    "swagger-ui-express": "^5.0.1",
    "url": "^0.11.4",
    "uuid": "^10.0.0",
    "yaml": "^2.8.1",
    "zod": "^3.24.1"
  },
  "_moduleAliases": {
    "@src": "src"
  }
}
